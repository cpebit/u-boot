/dts-v1/;

#include "rv1106.dtsi"
#include "rv1106-u-boot.dtsi"
#include <dt-bindings/input/input.h>

/ {
    model = "AAWireless 2B";
    compatible = "rockchip,aaw2b", "rockchip,rv1106";

    chosen {
        stdout-path = &uart2;
        u-boot,spl-boot-order = &spi_nand;
    };
};

&gpio1 {
    u-boot,dm-spl;
    status = "okay";
};

&pinctrl {
    keys {
        pwrbtn: pwrbtn {
            rockchip,pins = <RK_GPIO3 RK_PC5 RK_FUNC_GPIO &pcfg_pull_up>;
        };
    };
};

&sdmmc {
    status = "disabled";
};

&emmc {
    status = "disabled";
};

&otp {
    u-boot,dm-spl;
    status = "okay";
};

&uart2 {
    status = "okay";
};
